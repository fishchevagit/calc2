<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="CalcDB"
                   namespace="CalcDB.Models">

  <class name="SubsOperations" table="[SubsOperations]">
    <id name="Id">
      <generator class="native" />
    </id>
      
    <many-to-one name="Operation" class="Operation" column="OperationId" lazy="false"/>
    
    <many-to-one name="Subscription" class="Subscription" column="SubsId" lazy="false"/>

  </class>

</hibernate-mapping>